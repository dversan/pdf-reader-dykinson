{
  "buildCommand": "npx gulp generic && cp -r build/generic/build/* build/generic/web/",
  "outputDirectory": "build/generic/web",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npx gulp server",
  "rewrites": [
    {
      "source": "/",
      "destination": "/viewer.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "credentialless"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
