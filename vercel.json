{
  "buildCommand": "npx gulp generic",
  "outputDirectory": "build/generic",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npx gulp server",
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/viewer.html"
    },
    {
      "source": "/viewer.mjs",
      "destination": "/web/viewer.mjs"
    },
    {
      "source": "/viewer.css",
      "destination": "/web/viewer.css"
    },
    {
      "source": "/images/:path*",
      "destination": "/web/images/:path*"
    },
    {
      "source": "/locale/:path*",
      "destination": "/web/locale/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "credentialless"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
