{
  "buildCommand": "npx gulp generic",
  "outputDirectory": "build/generic/web",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "npx gulp server",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
